{
  "manifest_version": 2,
  "name": "SS_Replace_ChromeExtension",
  "description": "HTML replace and Redirector",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "512": "icons/icon512.png"
  },

  "background": { "scripts": ["background.js"] },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_start"
    }
  ],
  "options_page": "options.html",

  "permissions": [ "storage", "webRequest", "webRequestBlocking" ]
}
